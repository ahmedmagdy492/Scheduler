@model List<Scheduler.Data.Entities.Task>

@if (Model.Count > 0)
{
    foreach (var item in Model)
    {
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <a asp-action="Details" asp-controller="Tasks" asp-route-day="@item.CreationTime.Value.Day" asp-route-month="@item.CreationTime.Value.Month" asp-route-year="@item.CreationTime.Value.Year">@item.Title</a>
                </div>
            </div>
        </div>
    }
}
else
{
    <div class="col-md-12">
        <h3 class="text-center">No Results Found</h3>
    </div>
}